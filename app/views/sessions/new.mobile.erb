<html>
<center>
<h3>Log in</h3>
<table>
	<tr>
		<td><%= form_for(:session, url: sessions_path) do |f| %>
				<%= f.label :email %></td>
		<td><%= f.text_field :email %></td>
	</tr>
	<tr>
		<td><%= f.label :password %></td>
		<td><%= f.password_field :password %></td>
	</tr>
</table>
	<%= f.submit "Send", class: "btn" %><br><br>
	<% end %>
	<%= link_to 'New account', new_user_path %>
</center>
</html>

